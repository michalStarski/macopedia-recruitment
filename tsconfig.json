{
    "compilerOptions": {
        "module": "es6",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "alwaysStrict": true,
        "jsx": "react"
    },
    "include": [
        "src/**/*",
        "data/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
